{
  "expo": {
    "name": "VibeSpot",
    "slug": "vibespot",
    "version": "1.0.0",
    "orientation": "portrait",
    "main": "index.js",
    "icon": "./assets/images/VibeSpot-Logo-3.png",
    "scheme": "vibespot",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "backgroundColor": "#8b5cf6",
    "splash": {
      "backgroundColor": "#8b5cf6"
    },
    "ios": {
      "supportsTablet": true,
      "backgroundColor": "#8b5cf6",
      "bundleIdentifier": "com.anonymous.vibespot",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#8b5cf6",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/VibeSpot-Logo-3.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.vibespot"
    },
    "web": {
      "output": "server",
      "favicon": "./assets/images/favicon.png",
      "web": {
        "bundler": "metro"
      }
    },
    "plugins": [
      [
        "react-native-maps",
        {
          "iosGoogleMapsApiKey": "AIzaSyDIfMmh2V9_WfSBetHAq0qI8E5-TvCAYgs",
          "androidGoogleMapsApiKey": "AIzaSyDIfMmh2V9_WfSBetHAq0qI8E5-TvCAYgs"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow VibeSpot to access your photos to select event images.",
          "cameraPermission": "Allow VibeSpot to use your camera to take photos."
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ],
      [
        "expo-router",
        {
          "origin": "https://vibespot.expo.app"
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/VibeSpot-Logo-3.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#8b5cf6",
          "dark": {
            "backgroundColor": "#6d28d9"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {
        "origin": "https://vibespot.expo.app"
      },
      "eas": {
        "projectId": "50b28dab-3f92-4977-ab18-1d124e33669f"
      }
    }
  }
}
