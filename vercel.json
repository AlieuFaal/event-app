{
  "version": 2,
  "buildCommand": "cd apps/web && bun run build",
  "outputDirectory": "apps/web/.output/public",
  "installCommand": "bun install --frozen-lockfile",
  "framework": null,
  "functions": {
    "apps/web/.output/server/index.mjs": {
      "runtime": "nodejs20.x",
      "maxDuration": 60
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/apps/web/.output/server/index.mjs"
    }
  ]
}
